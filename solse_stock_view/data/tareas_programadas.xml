<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="0">

        <record id="recrear_stock" model="ir.cron">
            <field name="name">Recrear Stock</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
            <field name="user_id" ref="base.user_root" />
            <field name="state">code</field>
            <field name="model_id" ref="solse_stock_view.model_report_sstock_view" />
            <field name="code">model.recrear_stock()</field>
            <field name="active" eval="False" />
        </record>

        <record id="recrear_stock_lote" model="ir.cron">
            <field name="name">Recrear Stock Lote</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
            <field name="user_id" ref="base.user_root" />
            <field name="state">code</field>
            <field name="model_id" ref="solse_stock_view.model_report_sstock_lote_view" />
            <field name="code">model.recrear_stock()</field>
            <field name="active" eval="False" />
        </record>
    </data>
</odoo>